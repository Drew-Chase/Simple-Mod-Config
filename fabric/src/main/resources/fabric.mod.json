{
  "schemaVersion": 1,
  "id": "simplemodconfig",
  "version": "${version}",
  "name": "Simple Mod Config",
  "description": "A minecraft modding framework for creating configuration files for both client and server",
  "authors": [],
  "contact": {},
  "license": "Apache 2.0",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "chase.minecraft.architectury.simplemodconfig.fabric.TestModClientFabric"
    ],
    "main": [
      "chase.minecraft.architectury.simplemodconfig.fabric.SimpleModConfigFabric"
    ],
    "modmenu": ["chase.minecraft.architectury.simplemodconfig.fabric.TestModMenu"]
  },
  "mixins": [
    "simplemodconfig.mixins.json",
    "simplemodconfig-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.19",
    "minecraft": ">=1.19.4"
  }
}
